{
  "name": "etupro-app",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:emul": "npx supabase start",
    "start:watch": "npx supabase functions serve",
    "stop:emul": "npx supabase stop --no-backup",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "deploy": "npm ci && npm run build && firebase deploy",
    "supabase:db-pull": "npx supabase db pull --local --schema public,storage",
    "supabase:db-push": "npx supabase db push",
    "supabase:edge-push": "npx supabase functions deploy",
    "supabase:update-database-type": "npm run supabase:update-database-type-app && npm run supabase:update-database-type-edge",
    "supabase:update-database-type-app": "npx supabase gen types typescript --local > src/app/shared/models/database.types.ts",
    "supabase:update-database-type-edge": "npx supabase gen types typescript --local > supabase/functions/_shared/database.types.ts",
    "supabase:update-seed": "npx tsx seed.ts > supabase/seed.sql",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.10",
    "@angular/cdk": "17.3.10",
    "@angular/common": "17.3.10",
    "@angular/compiler": "17.3.10",
    "@angular/core": "17.3.10",
    "@angular/forms": "17.3.10",
    "@angular/material": "17.3.10",
    "@angular/platform-browser": "17.3.10",
    "@angular/platform-browser-dynamic": "17.3.10",
    "@angular/router": "17.3.10",
    "@supabase/supabase-js": "2.46.2",
    "emoji-toolkit": "8.0.0",
    "luxon": "3.4.4",
    "marked": "12.0.2",
    "ngx-markdown": "17.2.1",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "uuid": "10.0.0",
    "zone.js": "0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.8",
    "@angular-eslint/builder": "17.5.2",
    "@angular-eslint/eslint-plugin": "17.5.2",
    "@angular-eslint/eslint-plugin-template": "17.5.2",
    "@angular-eslint/schematics": "17.5.2",
    "@angular-eslint/template-parser": "17.5.2",
    "@angular/cli": "17.3.8",
    "@angular/compiler-cli": "17.3.10",
    "@snaplet/copycat": "5.0.0",
    "@snaplet/seed": "0.97.20",
    "@types/jasmine": "4.3.6",
    "@types/luxon": "3.4.2",
    "@types/pg": "8.11.6",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.57.0",
    "jasmine-core": "4.6.0",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "pg": "8.12.0",
    "supabase": "1.223.10",
    "tsx": "4.13.3",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": "20.11.1",
    "npm": "10.2.4"
  }
}
